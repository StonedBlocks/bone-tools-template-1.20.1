{
	"schemaVersion": 1,
	"id": "bonetools",
	"version": "${version}",
	"name": "Bone Tools",
	"description": "This is a mod that changes the way tools work by replacing any crafting recipe that has a stick in the crafting recipe with a bone!",
	"authors": [
		"TheNameIsLinus"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/bonetools/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.thenameislinus.bonetools.BoneTools"
		],
		"client": [
			"net.thenameislinus.bonetools.BoneToolsClient"
		],
		"fabric-datagen": [
			"net.thenameislinus.bonetools.BoneToolsDataGenerator"
		]
	},
	"mixins": [
		"bonetools.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}